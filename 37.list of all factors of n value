#include <stdio.h>
void findFactors(int n) {
    printf("Factors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    findFactors(n);
    return 0;
}
output
Enter a number: 5
Factors of 5 are: 1 5
